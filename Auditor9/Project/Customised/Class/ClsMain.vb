                NCatDesc = "Physical Stock"
            Case Ncat.PhysicalStockAdjustment
                NCatDesc = "Physical Stock Adjustment"
            Case Ncat.WorkOrder
                NCatDesc = "Work Order"
            Case Ncat.WorkInvoice
                NCatDesc = "Work Invoice"
            Case Else
                NCatDesc = bNCat
        End Select
        Return NCatDesc
    End Function
