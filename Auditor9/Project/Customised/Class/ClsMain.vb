    Public Shared Function FGetBomWithBomPattern(Process As String, ItemCategory As String, ItemGroup As String, Item As String,
                                 Dimension1 As String, Dimension2 As String,
                                 Dimension3 As String, Dimension4 As String,
                                 Size As String, RawMaterial As String, SkuCode As String, ItemBomPattern As String) As String
        Dim DtItemBomPattern As DataTable
        Dim DtItemBomList As DataTable
        Dim DrBomList As DataRow()
        Dim mQry As String = ""
        Dim bRate As Double = 0




        If SkuCode <> "" Then
            mQry = " Select * From Item With (NoLock) Where Code = '" & SkuCode & "'"
            Dim DtSku As DataTable = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)
            If DtSku.Rows.Count > 0 Then
                ItemCategory = AgL.XNull(DtSku.Rows(0)("ItemCategory"))
                ItemGroup = AgL.XNull(DtSku.Rows(0)("ItemGroup"))
                Item = AgL.XNull(DtSku.Rows(0)("BaseItem"))
                Dimension1 = AgL.XNull(DtSku.Rows(0)("Dimension1"))
                Dimension2 = AgL.XNull(DtSku.Rows(0)("Dimension2"))
                Dimension3 = AgL.XNull(DtSku.Rows(0)("Dimension3"))
                Dimension4 = AgL.XNull(DtSku.Rows(0)("Dimension4"))
                Size = AgL.XNull(DtSku.Rows(0)("Size"))
            End If
        End If

        If ItemBomPattern <> "" Then
            mQry = "Select '" & ItemBomPattern & "'Pattern "
            DtItemBomPattern = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)
        Else
            mQry = "SELECT * FROM ItemBomPattern With (NoLock) Where Item = '" & ItemCategory & "' 
                And Process = '" & Process & "' 
                Order By Sr "
            DtItemBomPattern = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)

            If DtItemBomPattern.Rows.Count = 0 Then
                mQry = "Select * FROM ItemBomPattern With (NoLock) Where Item Is Null 
                    And Process = '" & Process & "' 
                    Order By Sr "
                DtItemBomPattern = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)
            End If

            If DtItemBomPattern.Rows.Count = 0 Then
                mQry = "Select * FROM ItemBomPattern With (NoLock) Where Item Is Null 
                    And Process Is Null
                    Order By Sr "
                DtItemBomPattern = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)
            End If
        End If


        mQry = " Select * From Item With (NoLock) Where V_Type = '" & ItemV_Type.BOM & "'"
        DtItemBomList = AgL.FillData(mQry, IIf(AgL.PubServerName = "", AgL.GCn, AgL.GcnRead)).Tables(0)

        For I As Integer = 0 To DtItemBomPattern.Rows.Count - 1
            Dim PatternArr As String() = AgL.XNull(DtItemBomPattern.Rows(I)("Pattern")).ToString.Split(",")
            Dim FilterCriteria As String = "1=1"
            For J As Integer = 0 To PatternArr.Length - 1
                Select Case PatternArr(J)
                    Case "Item Category"
                        FilterCriteria += " And IsNull(ItemCategory,'') = '" & ItemCategory & "'"
                    Case "Item Group"
                        FilterCriteria += " And IsNull(ItemGroup,'') = '" & ItemGroup & "'"
                    Case "Item"
                        FilterCriteria += " And IsNull(Item,'') = '" & Item & "'"
                    Case "Dimension1"
                        FilterCriteria += " And IsNull(Dimension1,'') = '" & Dimension1 & "'"
                    Case "Dimension2"
                        FilterCriteria += " And IsNull(Dimension2,'') = '" & Dimension2 & "'"
                    Case "Dimension3"
                        FilterCriteria += " And IsNull(Dimension3,'') = '" & Dimension3 & "'"
                    Case "Dimension4"
                        FilterCriteria += " And IsNull(Dimension4,'') = '" & Dimension4 & "'"
                    Case "Size"
                        FilterCriteria += " And IsNull(Size,'') = '" & Size & "'"
                    Case "Raw Material"
                        FilterCriteria += " And IsNull(RawMaterial,'') = '" & RawMaterial & "'"
                End Select
            Next

            DrBomList = DtItemBomList.Select(FilterCriteria)
            If DrBomList.Length > 0 Then Exit For
        Next
        If DrBomList IsNot Nothing Then
            If DrBomList.Length > 0 Then
                FGetBomWithBomPattern = DrBomList(0)("Code")
            Else
                FGetBomWithBomPattern = ""
            End If
        Else
            FGetBomWithBomPattern = ""
        End If
    End Function
