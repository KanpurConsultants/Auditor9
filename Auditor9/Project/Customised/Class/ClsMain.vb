                    mQry = "Delete From ItemTypeSetting Where ItemType ='" & ItemTypeCode.RawProduct & "'"
                    AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
                End If

                If OverWriteOldValues Or NoRecordFound Then
                    mQry = "INSERT INTO ItemTypeSetting
                            (Code, ItemType)
                            VALUES('" & ItemTypeCode.RawProduct & "', '" & ItemTypeCode.RawProduct & "');
