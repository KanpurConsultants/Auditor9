                 " " & AgL.Chk_Text(IIf(AgL.StrCmp(AgL.XNull(.Rows(I)("PostAc")), "|PARTY|"), mNarrParty, mNarr)) & ",'" & mV_Type & "','" & mV_No & "','" & mV_Prefix & "'," &
                 " '" & mSite_Code & "','" & mDiv_Code & "'," & AgL.Chk_Text("") & "," &
                 " " & AgL.Chk_Text("") & "," & AgL.Chk_Text("") & "," &
                 " " & Val("") & "," & AgL.Chk_Text("") & "," & Val("") & "," & 0 & ",'Y','" & "" & "', " & AgL.Chk_Text(StrContraTextJV) & ", " & AgL.Chk_Text(mCostCenter) & ", " & AgL.Chk_Text(mLinkedSubCode) & ")"
                    AgL.Dman_ExecuteNonQry(mQry, Conn, Cmd)
                End If
            Next I
        End With

        If AgL.IsTableExist(bTableName.Replace("[", "").Replace("]", ""), IIf(AgL.PubServerName = "", Conn, AgL.GcnRead)) Then
            mQry = "Drop Table " + bTableName
            AgL.Dman_ExecuteNonQry(mQry, IIf(AgL.PubServerName = "", Conn, AgL.GcnRead))
        End If
    End Sub
    Public Shared Function FGetAgChargesValue(DtCalcHeaderData As DataTable,
                                     DtCalcLineData As DataTable, ByVal Charges As String,
                               ByVal RowIndex As Integer, ByVal ColumnIndex As LineColumnType) As String
        Dim i As Integer

        FGetAgChargesValue = ""

        For i = 0 To DtCalcHeaderData.Rows.Count - 1
            If AgL.StrCmp(DtCalcHeaderData.Rows(i)("Charges"), Charges) Then
                If ColumnIndex = LineColumnType.Percentage Then
                    Select Case UCase(DtCalcHeaderData.Rows(i)("Value_Type"))
                        Case "PERCENTAGE", "PERCENTAGE CHANGEABLE", "PERCENTAGE FROM COLUMN"
                            Return AgL.XNull(DtCalcLineData.Rows(RowIndex)(GetColNamePer(DtCalcHeaderData.Rows(i)("Charges"))))
                    End Select
                ElseIf ColumnIndex = LineColumnType.PostAc Then
                    Return AgL.XNull(DtCalcLineData.Rows(RowIndex)(GetColNamePostAc(DtCalcHeaderData.Rows(i)("Charges"))))
                ElseIf ColumnIndex = LineColumnType.ContraAc Then
                    Return AgL.XNull(DtCalcLineData.Rows(RowIndex)(GetColNameContraAc(DtCalcHeaderData.Rows(i)("Charges"))))
                Else
                    Return AgL.XNull(DtCalcLineData.Rows(RowIndex)(GetColName(DtCalcHeaderData.Rows(i)("Charges"))))
                End If
            End If
        Next
    End Function
    Shared Function GetColName(ByVal Column_Caption As String)
        GetColName = Column_Caption
    End Function
    Shared Function GetColNamePer(ByVal Column_Caption As String)
        GetColNamePer = Column_Caption & "@"
    End Function
    Shared Function GetColNamePostAc(ByVal Column_Caption As String)
        GetColNamePostAc = Column_Caption & "PostAc"
    End Function
    Shared Function GetColNameContraAc(ByVal Column_Caption As String)
        GetColNameContraAc = Column_Caption & "ContraAc"
    End Function
    Enum LineColumnType
        Percentage = 0
        Amount = 1
        PostAc = 2
        ContraAc = 3
    End Enum

