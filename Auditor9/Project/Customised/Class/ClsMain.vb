    Public Sub FSeedTable_ItemTypeSetting(Optional OverWriteOldValues As Boolean = False)
        Dim mQry As String
        Dim NoRecordFound As Boolean
        Try
            'TRADING PRODUCTS
            If AgL.Dman_Execute("Select Count(*) from ItemTypeSetting Where ItemType = '" & ItemTypeCode.TradingProduct & "' ", AgL.GcnMain).ExecuteScalar = 0 Then
                NoRecordFound = True
            Else
                NoRecordFound = False
            End If

            If OverWriteOldValues And Not NoRecordFound Then
                mQry = "Delete From ItemTypeSetting Where ItemType ='" & ItemTypeCode.TradingProduct & "'"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If

            If OverWriteOldValues Or NoRecordFound Then
                mQry = "INSERT INTO ItemTypeSetting
                        (Code, ItemType,IsSalesTaxBasedOnRate)
                        VALUES('" & ItemTypeCode.TradingProduct & "', '" & ItemTypeCode.TradingProduct & "',1);
                       "
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If

            mQry = "Update ItemTypeSetting Set IsApplicable_Barcode = 0 
                    Where ItemType='" & ItemTypeCode.TradingProduct & "' 
                    And Div_Code Is Null 
                    And IsApplicable_Barcode Is Null"
            AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)


            If ClsMain.IsScopeOfWorkContains(IndustryType.TextileIndustry) Then
                mQry = "Update ItemTypeSetting Set IsItemGroupLinkedWithItemCategory = 1 
                    Where ItemType='" & ItemTypeCode.TradingProduct & "' 
                    And Div_Code Is Null 
                    And IsItemGroupLinkedWithItemCategory Is Null"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            Else
                mQry = "Update ItemTypeSetting Set IsItemGroupLinkedWithItemCategory = 0 
                    Where ItemType='" & ItemTypeCode.TradingProduct & "' 
                    And Div_Code Is Null 
                    And IsItemGroupLinkedWithItemCategory Is Null"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If


            If OverWriteOldValues Then
                If AgL.Dman_Execute("Select Count(*) from ItemTypeSetting Where ItemType = '" & ItemTypeCode.OtherProduct & "' ", AgL.GcnMain).ExecuteScalar > 0 Then
                    mQry = "Delete From ItemTypeSetting Where ItemType ='" & ItemTypeCode.OtherProduct & "'"
