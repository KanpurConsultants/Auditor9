                               GridType As String)
        Dim mQry As String
        Dim DtTemp As DataTable
        Dim I As Integer, J As Integer


        Dim Col1Head As String = "Head"
        Dim Col1Mandatory As String = ""
        Dim Col1Value As String = "Value"
        Dim Col1BtnDetail As String = "Detail"
        Dim Col1HeadOriginal As String = "Head Original"
        Dim Col1LastValue As String = "Last Value"

        Try
            Dim ParentProcess As String = ""
            If Process <> "" Then
                ParentProcess = AgL.XNull(AgL.Dman_Execute(" Select Parent From SubGroup Where SubCode = '" & Process & "'", AgL.GCn).ExecuteScalar())
            End If

            If GridType = GridTypeConstants.VerticalGrid Then
                Dim mDglRowCount As Integer
                For I = 0 To DGL.Rows.Count - 1
                    DGL.Rows(I).Visible = False
                Next


                mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And IfNull(SettingGroup,'') = '" & SettingGroup & "' And IfNull(Process,'') = '" & Process & "' And GridName ='" & DGL.Name & "' "
                DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                If DtTemp.Rows.Count = 0 Then
                    mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And IfNull(SettingGroup,'') = '" & SettingGroup & "' And IfNull(Process,'') = '' And GridName ='" & DGL.Name & "' "
                    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                    If DtTemp.Rows.Count = 0 Then
                        mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And IfNull(NCat,'') = '" & NCat & "' And IfNull(Process,'') = '" & Process & "' And GridName ='" & DGL.Name & "' "
                        DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                        If DtTemp.Rows.Count = 0 Then
                            mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And IfNull(NCat,'') = '" & NCat & "' And IfNull(Process,'') = '" & ParentProcess & "' And GridName ='" & DGL.Name & "' "
                            DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                            If DtTemp.Rows.Count = 0 Then
                                mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And V_Type = '" & V_Type & "' And GridName ='" & DGL.Name & "' "
                                DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                If DtTemp.Rows.Count = 0 Then
                                    mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And GridName ='" & DGL.Name & "' "
                                    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                    'If DtTemp.Rows.Count = 0 Then
                                    '    mQry = "Select H.* from EntryHeaderUISetting H Where EntryName='" & FrmName & "' And GridName ='" & DGL.Name & "' "
                                    '    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                    'End If
                                End If
                            End If
                        End If
                    End If
                End If

                If DtTemp.Rows.Count > 0 Then
                    For I = 0 To DtTemp.Rows.Count - 1
                        For J = 0 To DGL.Rows.Count - 1
                            If AgL.XNull(DtTemp.Rows(I)("FieldName")) = DGL.Item(Col1HeadOriginal, J).Value Then
                                DGL.Rows(J).Visible = AgL.VNull(DtTemp.Rows(I)("IsVisible"))
                                If AgL.VNull(DtTemp.Rows(I)("IsVisible")) Then mDglRowCount += 1
                                DGL.Item(Col1Mandatory, J).Value = IIf(AgL.VNull(DtTemp.Rows(I)("IsMandatory")), "", "")
                                If AgL.XNull(DtTemp.Rows(I)("Caption")) <> "" Then
                                    DGL.Item(Col1Head, J).Value = AgL.XNull(DtTemp.Rows(I)("Caption"))
                                End If
                            End If
                        Next
                    Next
                End If
                If mDglRowCount = 0 Then DGL.Visible = False Else DGL.Visible = True
            ElseIf GridType = GridTypeConstants.HorizontalGrid Then
                Dim mDglColumnCount As Integer
                For I = 0 To DGL.Columns.Count - 1
                    DGL.Columns(I).Visible = False
                Next

                mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And IfNull(SettingGroup,'') = '" & SettingGroup & "' And IfNull(Process,'') = '" & Process & "' And GridName ='" & DGL.Name & "' "
                DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                If DtTemp.Rows.Count = 0 Then
                    mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And IfNull(SettingGroup,'') = '" & SettingGroup & "' And IfNull(Process,'') = '' And GridName ='" & DGL.Name & "' "
                    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                    If DtTemp.Rows.Count = 0 Then
                        mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And IfNull(NCat,'') = '" & NCat & "' And IfNull(Process,'') = '" & Process & "' And GridName ='" & DGL.Name & "' And IfNull(SettingGroup,'') = ''"
                        DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                        If DtTemp.Rows.Count = 0 Then
                            mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And IfNull(NCat,'') = '" & NCat & "' And IfNull(Process,'') = '" & ParentProcess & "' And GridName ='" & DGL.Name & "' And IfNull(SettingGroup,'') = ''"
                            DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                            If DtTemp.Rows.Count = 0 Then
                                mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And V_Type = '" & V_Type & "' And GridName ='" & DGL.Name & "' And Process Is Null "
                                DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                If DtTemp.Rows.Count = 0 Then
                                    mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And NCat = '" & NCat & "' And GridName ='" & DGL.Name & "' And Process Is Null "
                                    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                    'If DtTemp.Rows.Count = 0 Then
                                    '    mQry = "Select H.* from EntryLineUISetting H Where EntryName='" & FrmName & "' And GridName ='" & DGL.Name & "' "
                                    '    DtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
                                    'End If
                                End If
                            End If
                        End If
                    End If
                End If

                If DtTemp.Rows.Count > 0 Then
                    For I = 0 To DtTemp.Rows.Count - 1
                        For J = 0 To DGL.Columns.Count - 1
                            If AgL.XNull(DtTemp.Rows(I)("FieldName")) = DGL.Columns(J).Name Then
                                DGL.Columns(J).Visible = AgL.VNull(DtTemp.Rows(I)("IsVisible"))
                                DGL.Columns(J).ReadOnly = Not CType(AgL.VNull(DtTemp.Rows(I)("IsEditable")), Boolean)
                                'If DGL.Columns(J).ReadOnly = True Then DGL.Columns(J).DefaultCellStyle.BackColor = Color.Beige
                                If AgL.VNull(DtTemp.Rows(I)("IsVisible")) Then mDglColumnCount += 1
                                If Not IsDBNull(DtTemp.Rows(I)("DisplayIndex")) Then
                                    DGL.Columns(J).DisplayIndex = AgL.VNull(DtTemp.Rows(I)("DisplayIndex"))
                                End If
                                If AgL.XNull(DtTemp.Rows(I)("Caption")) <> "" Then
                                    DGL.Columns(J).HeaderText = AgL.XNull(DtTemp.Rows(I)("Caption"))
                                End If
                            End If
                        Next
                    Next
                End If
                If mDglColumnCount = 0 Then DGL.Visible = False Else DGL.Visible = True
            End If
        Catch ex As Exception
            MsgBox(ex.Message & " [ApplySchemeTypeSetting]")
        End Try
    End Sub
    Public Shared Function FGetSKUCode(Sno As String, ItemTypeCode As String,
                      ItemCategoryCode As String, ItemCategoryName As String,
                      ItemGroupCode As String, ItemGroupName As String,
                      ItemCode As String, ItemName As String,
                      Dimension1Code As String, Dimension1Name As String,
                      Dimension2Code As String, Dimension2Name As String,
                      Dimension3Code As String, Dimension3Name As String,
                      Dimension4Code As String, Dimension4Name As String,
                      SizeCode As String, SizeName As String,
                      MastItemCategoryCode As String,
                      MastItemGroupCode As String,
                      MastItemSpecification As String,
                      MastDimension1Code As String,
                      MastDimension2Code As String,
                      MastDimension3Code As String,
                      MastDimension4Code As String,
                      MastSizeCode As String
                      ) As String
        Try

            Dim mSkuName As String
            Dim mSkuCode As String
            Dim DrItemCategory As DataRow()
            Dim DrBaseItem As DataRow()
            Dim DrSKU As DataRow()
            Dim objCMain As New ClsMain(AgL)



            If ItemCategoryCode = MastItemCategoryCode _
           And ItemGroupCode = MastItemGroupCode _
           And Dimension1Code = MastDimension1Code _
           And Dimension2Code = MastDimension2Code _
           And Dimension3Code = MastDimension3Code _
           And Dimension4Code = MastDimension4Code _
           And SizeCode = MastSizeCode Then
                FGetSKUCode = ItemCode
            Else
                mSkuName = ""
                mSkuCode = ""
                If MastItemSpecification <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += MastItemSpecification
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += ItemCode
                End If

                If ItemGroupName <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += ItemGroupName
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += ItemGroupCode
                End If

                If ItemCategoryName <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += ItemCategoryName
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += ItemCategoryCode
                End If

                If ItemName <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += ItemName
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += ItemCode
                End If

                If Dimension1Name <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += Dimension1Name
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += Dimension1Code
                End If

                If Dimension2Name <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += Dimension2Name
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += Dimension2Code
                End If

                If Dimension3Name <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += Dimension3Name
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += Dimension3Code
                End If

                If Dimension4Name <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += Dimension4Name
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += Dimension4Code
                End If


                If SizeName <> "" Then
                    If mSkuName <> "" Then mSkuName += "-"
                    mSkuName += SizeName
                    If mSkuCode <> "" Then mSkuCode += ","
                    mSkuCode += SizeCode
                End If


                If Not mSkuCode.Contains(",") Then
                    FGetSKUCode = mSkuCode
                ElseIf ItemCode <> "" And Dimension1Code = "" And
                       Dimension2Code = "" And Dimension3Code = "" And
                       Dimension4Code = "" And SizeCode = "" Then
                    FGetSKUCode = ItemCode
                Else
                    'DrSKU = AgL.PubDtItem.Select("Description = '" & mSkuName & "'")
                    DrSKU = AgL.PubDtItem.Select(" IsNull(ItemCategory,'') = '" & ItemCategoryCode & "'
                                    And IsNull(ItemGroup,'') = '" & ItemGroupCode & "'
                                    And IsNull(BaseItem,'') = '" & ItemCode & "'
                                    And IsNull(Dimension1,'') = '" & Dimension1Code & "'
                                    And IsNull(Dimension2,'') = '" & Dimension2Code & "'
                                    And IsNull(Dimension3,'') = '" & Dimension3Code & "'
                                    And IsNull(Dimension4,'') = '" & Dimension4Code & "'
                                    And IsNull(Size,'') = '" & SizeCode & "'
                                    And IsNull(V_Type,'') = '" & ItemV_Type.SKU & "'")
                    If DrSKU.Length > 0 Then
                        FGetSKUCode = AgL.XNull(DrSKU(0)("Code"))
                    Else
                        If ClsMain.FGetSettings(SettingFields.SkuManagementApplicableYN, SettingType.General, AgL.PubDivCode, AgL.PubSiteCode, ItemTypeCode, "", ItemV_Type.SKU, "", "") = "1" Then
                            DrItemCategory = AgL.PubDtItem.Select("Code = '" & ItemCategoryCode & "'")
                            DrBaseItem = AgL.PubDtItem.Select("Code = '" & ItemCode & "'")
                            If DrItemCategory.Length > 0 Then
                                If AgL.Dman_Execute("SELECT Count(*)
                                        FROM Item I With (NoLock)
                                        WHERE I.V_Type = '" & ItemV_Type.SKU & "'
                                        AND IsNull(I.ItemCategory,'') = '" & ItemCategoryCode & "'
                                        AND IsNull(I.ItemGroup,'') = '" & ItemGroupCode & "'
                                        AND IsNull(I.BaseItem,'') = '" & ItemCode & "'
                                        AND IsNull(I.Dimension1,'') = '" & Dimension1Code & "'
                                        AND IsNull(I.Dimension2,'') = '" & Dimension2Code & "'
                                        AND IsNull(I.Dimension3,'') = '" & Dimension3Code & "'
                                        AND IsNull(I.Dimension4,'') = '" & Dimension4Code & "'
                                        AND IsNull(I.Size,'') = '" & SizeCode & "'
                                        ", AgL.GcnRead).ExecuteScalar() = 0 Then

                                    Dim bHsnCode As String = ""
                                    If DrBaseItem.Length > 0 Then
                                        If AgL.XNull(DrBaseItem(0)("HSN")) <> "" Then
                                            bHsnCode = AgL.XNull(DrBaseItem(0)("HSN"))
                                        Else
                                            bHsnCode = AgL.XNull(DrItemCategory(0)("HSN"))
                                        End If
                                    End If
                                    FGetSKUCode = objCMain.FSeedSingleIfNotExist_SKU("", mSkuName, AgL.XNull(DrItemCategory(0)("Unit")), ItemV_Type.SKU, ItemTypeCode, ItemCategoryCode, ItemGroupCode, ItemCode, Dimension1Code, Dimension2Code, Dimension3Code, Dimension4Code, SizeCode,
                                                                                     bHsnCode, AgL.PubDivCode, "", AgL.XNull(DrItemCategory(0)("MaintainStockYn")), "System Defined", "")
                                    ClsMain.FCreateItemDataTable()
                                Else
                                    MsgBox("Sku already exist with this combination for row no. " & Sno & " . Press refresh button.", MsgBoxStyle.Information)
                                End If
                            End If
                        Else
                            MsgBox(mSkuName & " does not exist for row no. " & Sno & " . Can't Continue", MsgBoxStyle.Information)
                        End If
                    End If
                End If
            End If
        Catch ex As Exception
            MsgBox(ex.Message & " In FCreateSKU")
        End Try
    End Function



