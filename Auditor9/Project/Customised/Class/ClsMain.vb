    Public Sub FSeedSingleIfNotExist_EntryHeaderUISetting(EntryName As String, NCat As String, GridName As String, FieldName As String, Optional IsVisible As Boolean = False, Optional IsMandatory As Boolean = False, Optional IsSystemDefined As Boolean = False, Optional Caption As String = "", Optional V_Type As String = "", Optional Process As String = "", Optional SettingGroup As String = "")
        Dim mQry As String
        Dim mMaxId As String
        Try
            mMaxId = AgL.GetMaxId("EntryHeaderUISetting", "Code", AgL.GcnMain, AgL.PubDivCode, AgL.PubSiteCode, 8, True, True, AgL.ECmd, AgL.Gcn_ConnectionString)

            If AgL.FillData("Select * from EntryHeaderUISetting Where EntryName='" & EntryName & "' And IfNull(NCat,'')='" & NCat & "'  And IfNull(V_Type,'')='" & V_Type & "' And IfNull(Process,'')='" & Process & "' And IfNull(SettingGroup,'')='" & SettingGroup & "' And GridName = '" & GridName & "' And FieldName='" & FieldName & "' ", AgL.GcnMain).tables(0).Rows.Count = 0 Then
                mQry = "Insert Into EntryHeaderUISetting(Code, EntryName, NCat, V_Type, Process, SettingGroup, GridName, FieldName, IsVisible, IsMandatory, IsSystemDefined, Caption)
                    Values(" & AgL.Chk_Text(mMaxId) & ", " & AgL.Chk_Text(EntryName) & ", " & AgL.Chk_Text(NCat) & ", " & AgL.Chk_Text(V_Type) & ", " & AgL.Chk_Text(Process) & ", " & AgL.Chk_Text(SettingGroup) & ", " & AgL.Chk_Text(GridName) & ", " & AgL.Chk_Text(FieldName) & ", " & AgL.VNull(IsVisible) & ", " & AgL.VNull(IsMandatory) & ", " & AgL.VNull(IsSystemDefined) & ", " & AgL.Chk_Text(Caption) & ")
                   "
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If
        Catch ex As Exception
            MsgBox(ex.Message & "  [FSeedSingleIfNotExist_EntryHeaderUISetting] While Seeding " & EntryName + " / " + NCat + " / " + GridName + " / " + FieldName)
        End Try
    End Sub
