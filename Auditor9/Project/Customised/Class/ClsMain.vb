    Public Shared Function RemoveNullFromDataTable(dtMain As DataTable)
        Dim I As Integer, J As Integer

        If dtMain.Rows.Count > 0 Then
            For I = 0 To dtMain.Rows.Count - 1
                For J = 0 To dtMain.Columns.Count - 1
                    'MsgBox(dtMain.Columns(J).ColumnName & "             " & dtMain.Columns(J).DataType.ToString)
                    If IsDBNull(dtMain.Rows(I)(J)) Then
                        Select Case dtMain.Columns(J).DataType.ToString.ToUpper
                            Case "SYSTEM.DATETIME"
                                dtMain.Rows(I)(J) = "01-Jan-1900"
                            Case "SYSTEM.DOUBLE", "SYSTEM.LONG", "SYSTEM.DECIMAL", "SYSTEM.SHORT", "SYSTEM.SINGLE", "SYSTEM.BYTE"
                                dtMain.Rows(I)(J) = "0.0"
                            Case "SYSTEM.INT32"
                                dtMain.Rows(I)(J) = "0"
                            Case "SYSTEM.BOOLEAN"
                                dtMain.Rows(I)(J) = "0"
                            Case Else
                                dtMain.Rows(I)(J) = " "
                        End Select
                    End If
                Next
            Next
        End If

        RemoveNullFromDataTable = dtMain
    End Function

