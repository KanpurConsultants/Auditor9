        Catch ex As Exception
        End Try

        Try
            mQry = "SELECT V1.DocId, V1.Sr, V1.PurchaseInvoiceDocId, V2.V_Sno, 
                    'UPDATE Cloth_SupplierSettlementInvoices Set PurchaseInvoiceDocIdSr = ' || Cast(V2.V_Sno As nvarchar) || ' Where DocId = ' || '''' || V1.DocId || '''' || 
                    ' And Sr = ' || Cast(V1.Sr As nvarchar) || ';' As Qry
                    FROM (
	                    SELECT L.DocID, L.Sr, L.PurchaseInvoiceDocId
	                    From Cloth_SupplierSettlementInvoices L 
	                    Where L.PurchaseInvoiceDocId Is Not Null And L.PurchaseInvoiceDocIdSr Is Null
                    ) AS V1
                    LEFT JOIN (
	                    SELECT L.DocId, L.V_SNo
	                    FROM PurchInvoice H 
	                    LEFT JOIN Ledger L ON H.DocId = L.DocId And H.Vendor = L.SubCode
                    ) AS V2 ON V1.PurchaseInvoiceDocId = V2.DocId	
                    Where V2.V_Sno Is Not Null "
            Dim DtSettlement As DataTable = AgL.FillData(mQry, AgL.GCn).Tables(0)
            For I As Integer = 0 To DtSettlement.Rows.Count - 1
