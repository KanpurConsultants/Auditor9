    Public Shared Function FReplacePubVariables(ByRef dtTable As DataTable, DivisionCode As String, SiteCode As String) As DataTable
        Dim I As Integer, J As Integer
        For I = 0 To dtTable.Rows.Count - 1
            For J = 0 To dtTable.Columns.Count - 1
                If AgL.XNull(dtTable.Rows(I)(J)) <> "" Then
                    dtTable.Rows(I)(J) = FReplacePubVariables(dtTable.Rows(I)(J), DivisionCode, SiteCode)
                End If
            Next J
        Next I

        FReplacePubVariables = dtTable
    End Function

    Public Shared Function FReplacePubVariables(ByRef mText As String, DivisionCode As String, SiteCode As String) As String
        Dim mQry As String
        Dim dtTemp As DataTable


        If mText.IndexOf("<") >= 0 And mText.IndexOf(">") > 0 Then

            mText = Replace(mText, "<CompanyCity>", AgL.PubJurisdictionCity)

            mQry = "Select D.*, Sg.DispName as DivisionName, Sg.Address, City.CityName, Sg.Pin PinCode, Sg.Mobile, Sg.Phone, Sg.Email,
                State.Description as StateName, State.ManualCode as StateCode, ST.RegistrationNo as SalesTaxNo, PAN.RegistrationNo as PanNo 
                From Division D 
                Left Join Subgroup Sg On D.Subcode = Sg.Subcode
                Left Join City on Sg.CityCode = City.CityCode
                Left Join State On City.State = State.Code
                Left Join SubgroupRegistration ST On Sg.Subcode = ST.Subcode and ST.RegistrationType = '" & SubgroupRegistrationType.SalesTaxNo & "'
                Left Join SubgroupRegistration PAN On Sg.Subcode = PAN.Subcode and PAN.RegistrationType = '" & SubgroupRegistrationType.PanNo & "'
                Where D.Div_Code = '" & DivisionCode & "'"
            dtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
            If dtTemp.Rows.Count > 0 Then
                mText = Replace(mText, "<DivisionName>", AgL.XNull(dtTemp.Rows(0)("DivisionName")))
                mText = Replace(mText, "<DivisionCityName>", AgL.XNull(dtTemp.Rows(0)("CityName")))
                mText = Replace(mText, "<DivisionAddress>", AgL.XNull(dtTemp.Rows(0)("Address")))
                mText = Replace(mText, "<DivisionPinCode>", AgL.XNull(dtTemp.Rows(0)("PinCode")))
                mText = Replace(mText, "<DivisionStateName>", AgL.XNull(dtTemp.Rows(0)("StateName")))
                mText = Replace(mText, "<DivisionStateCode>", AgL.XNull(dtTemp.Rows(0)("StateCode")))
                mText = Replace(mText, "<DivisionSalesTaxNo>", AgL.XNull(dtTemp.Rows(0)("SalesTaxNo")))
                mText = Replace(mText, "<DivisionPanNo>", AgL.XNull(dtTemp.Rows(0)("PanNo")))
                mText = Replace(mText, "<DivisionMobile>", AgL.XNull(dtTemp.Rows(0)("Mobile")))
                mText = Replace(mText, "<DivisionPhone>", AgL.XNull(dtTemp.Rows(0)("Phone")))
                mText = Replace(mText, "<DivisionEmail>", AgL.XNull(dtTemp.Rows(0)("Email")))
            End If




            mQry = "Select S.*, Sg.Name as SiteName, Sg.Address, City.CityName, Sg.Pin, 
                    State.Description as StateName, State.ManualCode as StateCode 
                    From SiteMast S 
                    Left Join Subgroup Sg On S.Subcode = Sg.Subcode
                    Left Join City on Sg.CityCode = City.CityCode
                    Left Join State On City.State = State.Code
                    Where S.Code = '" & SiteCode & "'"
            dtTemp = AgL.FillData(mQry, AgL.GCn).Tables(0)
            If dtTemp.Rows.Count > 0 Then
                mText = Replace(mText, "<SiteName>", AgL.XNull(dtTemp.Rows(0)("SiteName")))
                mText = Replace(mText, "<SiteCityName>", AgL.XNull(dtTemp.Rows(0)("CityName")))
                mText = Replace(mText, "<SiteAddress>", AgL.XNull(dtTemp.Rows(0)("Address")))
                mText = Replace(mText, "<SitePincode>", AgL.XNull(dtTemp.Rows(0)("Pin")))
                mText = Replace(mText, "<SiteStateName>", AgL.XNull(dtTemp.Rows(0)("StateName")))
                mText = Replace(mText, "<SiteStateCode>", AgL.XNull(dtTemp.Rows(0)("StateCode")))
            End If
        End If
        FReplacePubVariables = mText
    End Function

