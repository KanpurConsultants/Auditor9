    Public Shared Function IsPartyBlocked(PartyCode As String, NCat As String) As Boolean
        Dim dtTemp As DataTable
        Dim mQry As String

        mQry = "Select * from SubgroupBlockedTransactions With (NoLock) where Subcode = '" & PartyCode & "' And NCat = '" & NCat & "' "
        dtTemp = AgL.FillData(mQry, AgL.GCn).TABLES(0)
        If dtTemp.Rows.Count > 0 Then
            IsPartyBlocked = True
        Else
            IsPartyBlocked = False
        End If
    End Function

