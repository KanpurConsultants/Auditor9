    Private Sub FUpdateDates(bTableName As String, bPrimaryKey As String)
        Dim mQry As String = ""
        mQry = "PRAGMA table_info('" & bTableName & "');"
        Dim DtFields As DataTable = AgL.FillData(mQry, AgL.GCn).Tables(0)
        For J As Integer = 0 To DtFields.Rows.Count - 1
            If AgL.StrCmp(DtFields.Rows(J)("Type"), "DateTime") Then
                mQry = "SELECT " & bPrimaryKey & " As PrimaryKeyValue, 
                        Cast(" & DtFields.Rows(J)("Name") & " As nVarChar) As FieldValue
                        FROM " & bTableName & "
                        WHERE DATE(" & DtFields.Rows(J)("Name") & ") IS NULL 
                        AND " & DtFields.Rows(J)("Name") & " IS NOT NULL"
                Dim DtData As DataTable = AgL.FillData(mQry, AgL.GCn).Tables(0)
                For K As Integer = 0 To DtData.Rows.Count - 1
                    mQry = " Update " & bTableName & "
                        Set " & DtFields.Rows(J)("Name") & " = " & AgL.Chk_Date(AgL.RetDate(AgL.XNull(DtData.Rows(K)("FieldValue")))) & " 
                        Where " & bPrimaryKey & " = '" & DtData.Rows(K)("PrimaryKeyValue") & "'"
                    AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)
                Next
            End If
        Next
    End Sub
    Private Sub FYearEnd()
        Dim mTrans As String = ""
        Try
            AgL.ECmd = AgL.GCn.CreateCommand
            AgL.ETrans = AgL.GCn.BeginTransaction(IsolationLevel.ReadCommitted)
            AgL.ECmd.Transaction = AgL.ETrans
            mTrans = "Begin"
            FrmYearClosing.FProcess1920(AgL.GCn, AgL.ECmd, AgL)
            FrmYearClosing.FProcess2021(AgL.GCn, AgL.ECmd, AgL)
            FrmYearClosing.FProcess2022(AgL.GCn, AgL.ECmd, AgL)
            AgL.ETrans.Commit()
            mTrans = "Commit"
            'MsgBox("Process Completed...!", MsgBoxStyle.Information)
        Catch ex As Exception
            AgL.ETrans.Rollback()
            MsgBox(ex.Message)
        End Try
    End Sub

    Public Shared Function ImageToStream(ByVal fileName As String) As Byte()
        Dim stream As New MemoryStream()
tryagain:
        Try
            Dim image As New Bitmap(fileName)
            image.Save(stream, System.Drawing.Imaging.ImageFormat.Jpeg)
        Catch ex As Exception
            GoTo tryagain
        End Try
        Return stream.ToArray()
    End Function
    Public Shared Sub FifoAdjustSale(Optional IsSilent As Boolean = False)
        Dim mQry As String = ""
        Dim bAdjQty As Double = 0

        mQry = "SELECT L.DocID, L.TSr, L.Sr, L.Item, IsNull(L.Qty_Iss,0) - IsNull(VStockAdj.TotalAdjQty,0) As Qty
                FROM Stock L 
                LEFT JOIN Voucher_Type Vt ON L.V_Type = Vt.V_Type
                LEFT JOIN (
	                SELECT Sad.StockOutDocID, Sad.StockOutTSr, Sad.StockOutSr, Sum(Sad.AdjQty) AS TotalAdjQty
	                FROM StockAdj Sad
	                GROUP BY Sad.StockOutDocID, Sad.StockOutTSr, Sad.StockOutSr
                ) AS VStockAdj ON L.DocID = VStockAdj.StockOutDocID AND L.TSr = VStockAdj.StockOutTSr AND L.Sr = VStockAdj.StockOutSr
                WHERE IsNull(L.Qty_Iss,0) - IsNull(VStockAdj.TotalAdjQty,0) > 0
                And L.Site_Code = '" & AgL.PubSiteCode & "'
                And L.Div_Code = '" & AgL.PubDivCode & "'
                Order BY L.V_Date "
        Dim DtStockOut As DataTable = AgL.FillData(mQry, AgL.GCn).Tables(0)

        mQry = "SELECT L.DocID, L.TSr, L.Sr, L.Item, IsNull(L.Qty_Rec,0) - IsNull(VStockAdj.TotalAdjQty,0) As Qty
                FROM Stock L 
                LEFT JOIN Voucher_Type Vt ON L.V_Type = Vt.V_Type
                LEFT JOIN Item I On L.Item = I.Code
                LEFT JOIN (
	                SELECT Sad.StockInDocID, Sad.StockInTSr, Sad.StockInSr, Sum(Sad.AdjQty) AS TotalAdjQty
	                FROM StockAdj Sad
	                GROUP BY Sad.StockInDocID, Sad.StockInTSr, Sad.StockInSr
                ) AS VStockAdj ON L.DocID = VStockAdj.StockInDocID AND L.TSr = VStockAdj.StockInTSr AND L.Sr = VStockAdj.StockInSr
                WHERE IsNull(L.Qty_Rec,0) - IsNull(VStockAdj.TotalAdjQty,0) > 0 
                And L.Site_Code = '" & AgL.PubSiteCode & "'
                And L.Div_Code = '" & AgL.PubDivCode & "'
                Order BY L.V_Date "
        Dim DtStockIn As DataTable = AgL.FillData(mQry, AgL.GCn).Tables(0)

        For I As Integer = 0 To DtStockOut.Rows.Count - 1
            For J As Integer = 0 To DtStockIn.Rows.Count - 1
                If AgL.XNull(DtStockOut.Rows(I)("Item")) = AgL.XNull(DtStockIn.Rows(J)("Item")) And
                        DtStockOut.Rows(I)("Qty") > 0 And AgL.VNull(DtStockIn.Rows(J)("Qty")) > 0 Then
                    If AgL.VNull(DtStockOut.Rows(I)("Qty")) <= AgL.VNull(DtStockIn.Rows(J)("Qty")) Then
                        bAdjQty = AgL.VNull(DtStockOut.Rows(I)("Qty"))
                    Else
                        bAdjQty = AgL.VNull(DtStockIn.Rows(J)("Qty"))
                    End If

                    If AgL.Dman_Execute(" Select Count(*) From StockAdj 
                            Where StockInDocID = " & AgL.Chk_Text(AgL.XNull(DtStockIn.Rows(J)("DocId"))) & "
                            And StockInTSr = " & AgL.VNull(DtStockIn.Rows(J)("TSr")) & "
                            And StockInSr = " & AgL.VNull(DtStockIn.Rows(J)("Sr")) & "
                            And StockOutDocID = " & AgL.Chk_Text(AgL.XNull(DtStockOut.Rows(I)("DocId"))) & "
                            And StockOutTSr = " & AgL.VNull(DtStockOut.Rows(I)("TSr")) & "
                            And StockOutSr = " & AgL.VNull(DtStockOut.Rows(I)("Sr")) & "", AgL.GCn).ExecuteScalar() <> 0 Then
                        mQry = " Delete From StockAdj 
                                Where StockInDocID = " & AgL.Chk_Text(AgL.XNull(DtStockIn.Rows(J)("DocId"))) & "
                                And StockInTSr = " & AgL.VNull(DtStockIn.Rows(J)("TSr")) & "
                                And StockInSr = " & AgL.VNull(DtStockIn.Rows(J)("Sr")) & "
                                And StockOutDocID = " & AgL.Chk_Text(AgL.XNull(DtStockOut.Rows(I)("DocId"))) & "
                                And StockOutTSr = " & AgL.VNull(DtStockOut.Rows(I)("TSr")) & "
                                And StockOutSr = " & AgL.VNull(DtStockOut.Rows(I)("Sr")) & ""
                        AgL.Dman_ExecuteNonQry(mQry, AgL.GCn, AgL.ECmd)
                    End If

                    mQry = " INSERT INTO StockAdj (StockInDocID, StockInTSr, StockInSr, StockOutDocID, StockOutTSr, StockOutSr, AdjQty)
                            VALUES (" & AgL.Chk_Text(AgL.XNull(DtStockIn.Rows(J)("DocId"))) & ", 
                            " & AgL.VNull(DtStockIn.Rows(J)("TSr")) & ", 
                            " & AgL.VNull(DtStockIn.Rows(J)("Sr")) & ", 
                            " & AgL.Chk_Text(AgL.XNull(DtStockOut.Rows(I)("DocId"))) & ", 
                            " & AgL.VNull(DtStockOut.Rows(I)("TSr")) & ", 
                            " & AgL.VNull(DtStockOut.Rows(I)("Sr")) & ", 
                            " & AgL.VNull(bAdjQty) & ") "
                    AgL.Dman_ExecuteNonQry(mQry, AgL.GCn, AgL.ECmd)

                    DtStockOut.Rows(I)("Qty") = AgL.VNull(DtStockOut.Rows(I)("Qty")) - bAdjQty
                    DtStockIn.Rows(J)("Qty") = AgL.VNull(DtStockIn.Rows(J)("Qty")) - bAdjQty
                End If
            Next
        Next
        If IsSilent = False Then
            MsgBox("Process Completed.", MsgBoxStyle.Information)
        End If
    End Sub
