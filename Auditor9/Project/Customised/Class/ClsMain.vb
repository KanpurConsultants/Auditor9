    Public Shared Function GetAveragePaymentDays(Subcode As String, Optional UpdateMaster As Boolean = False)
        Dim mQry As String
        Dim I As Integer
        Dim J As Integer
        Dim mAverageDaysWeightage As Double
        Dim mAPDays As Double

        Dim DtBill As DataTable
        Dim DtPayment As DataTable
        Dim mBillAmt As Double
        Dim mPaymentAmt As Double
        Dim mPaymentDays As Integer
        Dim mAdjustmentAmount As Double


        Try
            mQry = "Drop Table #APD"
            AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)
        Catch ex As Exception
        End Try

        mQry = " CREATE Temporary TABLE #APD
                    (BillDocId  nVarchar(21), 
                     V_Date DateTime,
                     BillAmount Float,
                     BillBalance Float,
                     PaymentDocID nVarchar(21),
                     PaymentDate DateTime,
                     PaymentAmount Float,
                     PaymentBalance Float,
                     PaymentDays Float,
                     AdjustmentAmount Float,                        
                     APDays Float
                    ); "
        AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)


        mQry = "Select L.DocID, IfNull(L.EffectiveDate, L.V_Date) as V_Date, L.AmtDr as Amount
                From Ledger L
                Where L.AmtDr > 0 And L.Subcode = '" & Subcode & "' Order By IfNull(L.EffectiveDate, L.V_Date) "
        DtBill = AgL.FillData(mQry, AgL.GCn).Tables(0)

        mQry = "Select L.DocID, IfNull(L.EffectiveDate, L.V_Date) as V_Date, L.AmtCr  as Amount 
                From Ledger L
                Where L.AmtCr > 0 And L.Subcode = '" & Subcode & "'  Order by IfNull(L.EffectiveDate, L.V_Date) "
        DtPayment = AgL.FillData(mQry, AgL.GCn).Tables(0)

        'If DtPayment.Rows.Count > 0 Then
        '    mPaymentAmt = DtPayment.Rows(0)("Amount")
        'End If

        I = 0 : J = -1
        While I < DtBill.Rows.Count
            mAPDays = 0
            mBillAmt = DtBill.Rows(I)("Amount")





            While mBillAmt > 0

                If mPaymentAmt = 0 Then
                    J = J + 1
                    If J < DtPayment.Rows.Count Then
                        mPaymentAmt = AgL.VNull(DtPayment.Rows(J)("Amount"))
                    End If
                End If


                If mBillAmt < mPaymentAmt Then
                    mPaymentAmt = mPaymentAmt - mBillAmt
                    mAdjustmentAmount = mBillAmt
                    mBillAmt = 0

                    If J > DtPayment.Rows.Count - 1 Then
                        mPaymentDays = DateDiff(DateInterval.Day, AgL.XNull(DtBill.Rows(I)("V_Date")), DateTime.Now)
                        mBillAmt = 0
                    Else
                        mPaymentDays = DateDiff(DateInterval.Day, AgL.XNull(DtBill.Rows(I)("V_Date")), AgL.XNull(DtPayment.Rows(J)("V_Date")))
                    End If

                    mAverageDaysWeightage = (mAdjustmentAmount / AgL.VNull(DtBill.Rows(I)("Amount"))) * mPaymentDays
                    mAPDays += mAverageDaysWeightage
                End If


                If mBillAmt >= mPaymentAmt Then
                    mBillAmt = mBillAmt - mPaymentAmt
                    mAdjustmentAmount = mPaymentAmt
                    mPaymentAmt = 0

                    If J > DtPayment.Rows.Count - 1 Then
                        mPaymentDays = DateDiff(DateInterval.Day, AgL.XNull(DtBill.Rows(I)("V_Date")), DateTime.Now)
                        mAdjustmentAmount = mBillAmt
                        mBillAmt = 0
                    Else
                        mPaymentDays = DateDiff(DateInterval.Day, AgL.XNull(DtBill.Rows(I)("V_Date")), AgL.XNull(DtPayment.Rows(J)("V_Date")))
                    End If

                    mAverageDaysWeightage = (mAdjustmentAmount / AgL.VNull(DtBill.Rows(I)("Amount"))) * mPaymentDays
                    mAPDays += mAverageDaysWeightage
                End If





            End While

            If J < DtPayment.Rows.Count Then
                mQry = "Insert Into #APD (BillDocId, V_Date, BillAmount, BillBalance, 
                            PaymentDocID, PaymentDate, PaymentAmount, PaymentBalance, 
                            AdjustmentAmount, PaymentDays, APDays)
                            Values (" & AgL.Chk_Text(DtBill.Rows(I)("DocID")) & ", " & AgL.Chk_Date(DtBill.Rows(I)("V_Date")) & ",
                            " & AgL.VNull(DtBill.Rows(I)("Amount")) & ", " & mBillAmt & ",
                            " & AgL.Chk_Text(DtPayment.Rows(J)("DocID")) & ", " & AgL.Chk_Date(DtPayment.Rows(J)("V_Date")) & ",
                            " & AgL.VNull(DtPayment.Rows(J)("Amount")) & ", " & mPaymentAmt & ", " & mAdjustmentAmount & ", " & mPaymentDays & ", " & mAPDays & "
                            )"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)
            End If

            If J >= DtPayment.Rows.Count Then
                mQry = "Insert Into #APD (BillDocId, V_Date, BillAmount, BillBalance, 
                            PaymentDocID, PaymentDate, PaymentAmount, PaymentBalance, 
                            AdjustmentAmount, PaymentDays, APDays)
                            Values (" & AgL.Chk_Text(DtBill.Rows(I)("DocID")) & ", " & AgL.Chk_Date(DtBill.Rows(I)("V_Date")) & ",
                            " & AgL.VNull(DtBill.Rows(I)("Amount")) & ", " & mBillAmt & ",
                            Null, Null,
                            Null, " & mPaymentAmt & ", " & mAdjustmentAmount & ", " & mPaymentDays & ", " & mAPDays & "
                            )"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)

            End If

            I = I + 1
        End While

        mQry = "Select IfNull(Sum(ApDays),0) / (Case When Count(BillDocId) = 0 then 1 Else Count(BillDocID) End) From #APD "

        mAPDays = AgL.Dman_Execute(mQry, AgL.GCn).ExecuteScalar()

        'mAPDays = Math.Round(mAPDays / DtBill.Rows.Count, 0)

        If UpdateMaster Then
            mQry = "Update Subgroup Set AveragePaymentDays = " & mAPDays & " where Subcode = '" & Subcode & "' "
            AgL.Dman_ExecuteNonQry(mQry, AgL.GCn)
        End If

        GetAveragePaymentDays = mAPDays
    End Function
