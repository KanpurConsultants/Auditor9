            If Code = "" Then mMaxId = AgL.GetMaxId("Item", "Code", AgL.GcnMain, AgL.PubDivCode, AgL.PubSiteCode, 8, True, True, AgL.ECmd, AgL.Gcn_ConnectionString)

            If Code <> "" Then
                If AgL.FillData("Select Code from Item Where Code='" & Code & "'", AgL.GcnMain).tables(0).Rows.Count = 0 Then
                    mFoundRecord = False
                End If
            Else
                dtTemp = AgL.FillData("Select Code from Item Where Description='" & Description & "'", AgL.GcnMain).tables(0)
                If dtTemp.Rows.Count > 0 Then
                    Code = dtTemp.Rows(0)("Code")
                Else
                    mFoundRecord = False
                End If
            End If
            If mFoundRecord = False Then
                mQry = "
                    INSERT INTO Item (Code, ManualCode, Description, Unit, ItemGroup, ItemCategory, ItemType, V_Type, HSN, EntryBy, EntryDate, Div_Code, SalesTaxPostingGroup, MaintainStockYn, LockText, Parent)
                    VALUES ('" & mMaxId & "', '" & mMaxId & "', " & AgL.Chk_Text(Description) & ", " & AgL.Chk_Text(Unit) & ", " & AgL.Chk_Text(ItemGroup) & ", " & AgL.Chk_Text(ItemCategory) & ", " & AgL.Chk_Text(ItemType) & ", " & AgL.Chk_Text(V_Type) & ", " & AgL.Chk_Text(HSN) & ",  " & AgL.Chk_Text(AgL.PubUserName) & ", " & AgL.Chk_Date(AgL.PubLoginDate) & ", " & AgL.Chk_Text(Div_Code) & ", " & AgL.Chk_Text(SalesTaxPostingGroup) & ", " & IIf(MaintainStockYn, 1, 0) & ", " & AgL.Chk_Text(LockText) & ", " & AgL.Chk_Text(Parent) & ")
