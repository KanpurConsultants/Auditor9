    Private Sub FSeedTable_Menus()
        Dim mQry As String
        Try
            'Start of This section will be removed after all customer updation
            mQry = " Delete From Menus Where Parent Is Null "
            AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)

            mQry = "INSERT INTO Menus (MnuModule, MnuName, MnuText, Parent, ShortCutKey, IsVisible)
                    SELECT U.MnuModule, U.MnuName, U.MnuText, U.Parent, NULL AS ShortCutKey, 1 As IsVisible
                    FROM User_Permission U
                    LEFT JOIN Menus M ON U.MnuModule = M.MnuModule AND U.MnuName = M.MnuName
                      AND U.MnuText = M.MnuText
                    WHERE U.UserName = 'Sa' AND U.Parent <>'' And IfNull(Active,'Y') = 'Y' 
                    And IsNull(U.IsParent,0) = 0
                    And M.MnuName Is Null "
            AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            'End of This section will be removed after all customer updation

            Dim Mdi As New MDIMain
            Dim Mnu As Object
            For Each Mnu In Mdi.Controls
                If Mnu.GetType.ToString = "System.Windows.Forms.MenuStrip" Then
                    FRotateAllMenuItems(Mnu, Mnu.Name, "Customise", "Customise", AgL.GcnMain)
                End If
            Next

            FAssignMenuShortCut(Mdi.MnuSalesEntry.Name, "+Alt+S")
            FAssignMenuShortCut(Mdi.MnuPurchaseEntry.Name, "+Alt+P")
            FAssignMenuShortCut(Mdi.MnuDetailTrialBalance_Aadhat.Name, "+Alt+L")
            FAssignMenuShortCut(Mdi.MnuPaymentEntry.Name, "+Alt+M")
            FAssignMenuShortCut(Mdi.MnuReceiptEntry.Name, "+Alt+R")
            FAssignMenuShortCut(Mdi.MnuStockReport.Name, "+Alt+T")
        Catch ex As Exception
            MsgBox(ex.Message & "  [FSeedTable_Menus]")
        End Try
    End Sub
    Public Function FRotateAllMenuItems(ByRef MnuStrp As MenuStrip, ByVal StrMnuMain As String, ByVal StrModule As String, ByVal StrParent As String,
    ByVal GCnCmd As Object) As Integer
        Dim TSI_Main As ToolStripItem
        Dim TSMI_Main As ToolStripMenuItem

        For Each TSI_Main In MnuStrp.Items
            If TSI_Main.GetType.ToString = "System.Windows.Forms.ToolStripMenuItem" Then
                TSMI_Main = TSI_Main
                FRotateAllMenuItems(TSMI_Main.DropDownItems, StrMnuMain, StrModule, TSMI_Main.Name, GCnCmd)
            End If
        Next
    End Function
    Public Function FRotateAllMenuItems(ByRef Menus As ToolStripItemCollection, ByVal StrMnuMain As String,
                                        ByVal StrModule As String, ByVal StrParent As String,
                                        ByVal GCnCmd As Object) As Integer
        Dim TSI_Main As ToolStripItem
        Dim TSMI_Main As ToolStripMenuItem

        For Each TSI_Main In Menus
            If Trim(TSI_Main.Text) <> "" Then
                If TSI_Main.GetType.ToString = "System.Windows.Forms.ToolStripMenuItem" Then
                    TSMI_Main = TSI_Main
                    If TSMI_Main.DropDownItems.Count = 0 Then
                        FInsertUP(StrParent, TSI_Main.Text, TSI_Main.Name, StrModule)
                    End If
                    FRotateAllMenuItems(TSMI_Main.DropDownItems, StrMnuMain, StrModule, TSMI_Main.Name, GCnCmd)
                End If
            End If
        Next
    End Function
    Public Sub FInsertUP(ByVal StrParent As String, ByVal StrMnuText As String, ByVal StrMnuName As String,
                         ByVal StrMnuModule As String)
        If AgL.VNull(AgL.Dman_Execute("Select Count(*) From Menus Where MnuName = '" & StrMnuName & "'", AgL.GcnMain).ExecuteScalar()) = 0 Then
            AgL.Dman_ExecuteNonQry("Insert Into Menus(Parent,MnuText,MnuName,MnuModule) Values " &
                        "('" & StrParent & "','" & Replace(StrMnuText, "&", "") & "','" & StrMnuName & "','" & StrMnuModule & "')", AgL.GcnMain)
        End If
    End Sub

