    Public Sub FSeedTable_SaleInvoiceSetting(OverWriteOldValues As Boolean)
        Dim mQry As String
        Dim NoRecordFound As Boolean
        Try

            If AgL.Dman_Execute("Select Count(*) from SaleInvoiceSetting Where V_Type In (Select V_Type From Voucher_Type Where NCat = '" & Ncat.SaleInvoice & "') Or V_Type Is Null", AgL.GcnMain).ExecuteScalar = 0 Then
                NoRecordFound = True
            Else
                NoRecordFound = False
            End If

            If OverWriteOldValues And Not NoRecordFound Then
                mQry = "Delete From SaleInvoiceSetting Where V_Type In (Select V_Type From Voucher_Type Where NCat = '" & Ncat.SaleInvoice & "')  Or V_Type Is Null"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If

            If OverWriteOldValues Or NoRecordFound Then
                mQry = "INSERT INTO SaleInvoiceSetting
                        (Code, FilterInclude_Nature, Default_TermsAndConditions)
                        VALUES('" & Ncat.SaleInvoice & "', '+Customer+Cash', '(1) Payment within <CreditDays> days from date of Invoice. (2) Overdue Interest @ <Default_DebtorsInterestRate> P.A. after due date. (3) Goods dispatched by transport at the owner risk. (4) All disputes are subject to <CompanyCity> jurisdiction only.');
                       "
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
