                mQry = "UPDATE Voucher_Type SET MnuAttachedInModule = 'Customised' 	, MnuName = 'MnuJournalEntry' , MnuText = 'Journal Entry' WHERE V_Type = 'JV'"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)

                mQry = "UPDATE AcGroup SET Nature = 'Expenses' WHERE GroupName = 'SALERY A/C' AND Nature IS NULL"
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)

                mQry = "UPDATE Subgroup
                        SET Subgroup.Nature = V1.Nature
                        FROM (
	                        SELECT Sg.Subcode, Ag.Nature
	                        FROM Subgroup Sg 
	                        LEFT JOIN AcGroup Ag ON Sg.GroupCode = Ag.GroupCode
	                        WHERE Ag.GroupName = 'SALERY A/C'  
	                        AND Sg.Nature IS NULL
                            And Ag.Nature Is Not Null
                        ) AS V1 WHERE Subgroup.Subcode = V1.SubCode "
                AgL.Dman_ExecuteNonQry(mQry, AgL.GcnMain)
            End If



        Catch ex As Exception
            MsgBox(ex.Message & " In FCorrectionQueries")
        End Try
    End Sub

    Public Shared Function FDivisionNameForCustomization(Optional NoOfCharsFromLeft As Integer = 100) As String
        Dim mQry As String
        Dim DivName As String
        mQry = "Select Upper(Substr(Name,1," & NoOfCharsFromLeft & ")) From Subgroup With (NoLock) Where Subcode = 'D' "
        If AgL.PubServerName <> "" Then
            DivName = AgL.Dman_Execute(mQry, AgL.GcnMain).ExecuteScalar()
        Else
            DivName = AgL.Dman_Execute(mQry, AgL.GCn).ExecuteScalar()
        End If

        FDivisionNameForCustomization = DivName
    End Function

